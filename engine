server <- function(input, output) {
  datasetInput <- reactive({
    inFile <- input$file1
    if (is.null(inFile))
      return(NULL)
    read.csv(
      inFile$datapath,
      header = input$header,
      sep = input$sep,
      quote = input$quote
    )
  })
  output$Chtoposchit <- renderPrint("мы посчитали среднее, медиану, а также максималное и минимальное значение")
  output$contents <- renderPrint({
    dataset <- datasetInput
    summary(dataset)
    output$tablica <- renderTable( read.csv(inFile$datapath, header=input$header, sep=input$sep, quote=input$quote))
  })
}
